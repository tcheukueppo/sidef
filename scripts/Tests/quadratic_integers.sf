func is_gaussian_quadratic_pseudoprime(n) {

    return false if (n <= 1)
    return true  if (n <= 3)

    static x = Quadratic(1, -1, -2)

    given (n%8) {
        when ([1,3]) {
            var t = x.powmod(n-1, n)
            (t.a==1 && t.b==0)
        }
        when ([5, 7]) {
            var t = x.powmod(n+1, n)
            (t.a==3 && t.b==0)
        }
        else {
            false
        }
    }
}

